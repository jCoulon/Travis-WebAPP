div(ng-controller="dashboardIndicateur as dashboardCtrl")
    input(ng-model='dashboardCtrl.titre', placeholder='Titre', required='')
    input(ng-model='dashboardCtrl.type', palceholder='type', required='')
    button(ng-click='dashboardCtrl.ajouterNouvelIndicateurBloc()') Ajouter un nouvel indicateur {{dashboardOptions}}
    indicateur-area(options='dashboardCtrl.indicateurBlocsPanel'  on-supp='dashboardCtrl.supprimerIndicateurBloc(indicateurBloc)')
div#addnote
    h3 Ajouter une note
    input#titrenote(type="text")
    textarea#textnote
    label(for="sharenote") Partag√©e
    input#sharenote(type="checkbox")
    input#shareuser(type="text")
    button(type="button") +
    input(type="submit" ng-click="alert()" value="Ajouter")
